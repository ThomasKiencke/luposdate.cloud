INTERMEDIATE_BAG_8f = FILTER INTERMEDIATE_BAG_8 by lupos.cloud.pig.udfs.BloomfilterUDF(bloom1.$0, $0) AND  lupos.cloud.pig.udfs.BloomfilterUDF(bloom2.$0, $0) AND  lupos.cloud.pig.udfs.BloomfilterUDF(bloom3.$0, $0) AND  lupos.cloud.pig.udfs.BloomfilterUDF(bloom4.$0, $0) AND  lupos.cloud.pig.udfs.BloomfilterUDF(bloom5.$0, $0) AND  lupos.cloud.pig.udfs.BloomfilterUDF(bloom6.$0, $0) AND  lupos.cloud.pig.udfs.BloomfilterUDF(bloom7.$0, $0) AND  lupos.cloud.pig.udfs.BloomfilterUDF(bloom0.$0, $0);

INTERMEDIATE_BAG_9 = JOIN INTERMEDIATE_BAG_0f BY $0, INTERMEDIATE_BAG_1f BY $0, INTERMEDIATE_BAG_2f BY $0, INTERMEDIATE_BAG_3f BY $0, INTERMEDIATE_BAG_4f BY $0, INTERMEDIATE_BAG_5f BY $0, INTERMEDIATE_BAG_6f BY $0, INTERMEDIATE_BAG_7f BY $0, INTERMEDIATE_BAG_8f BY $0 PARALLEL 5;

-- Projection: ?author, ?inproc, ?title, ?ee, ?url, ?booktitle, ?yr, ?proc, ?page
INTERMEDIATE_BAG_10 = FOREACH INTERMEDIATE_BAG_9 GENERATE $2, $0, $6, $10, $14, $4, $16, $8, $12;

