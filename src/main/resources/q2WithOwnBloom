-- Join over ?inproc

INTERMEDIATE_BAG_0f = FILTER INTERMEDIATE_BAG_0 by lupos.cloud.pig.udfs.BloomfilterUDF(bloom1.$0, $0) AND  lupos.cloud.pig.udfs.BloomfilterUDF(bloom2.$0, $0) AND  lupos.cloud.pig.udfs.BloomfilterUDF(bloom3.$0, $0) AND  lupos.cloud.pig.udfs.BloomfilterUDF(bloom4.$0, $0) AND  lupos.cloud.pig.udfs.BloomfilterUDF(bloom5.$0, $0) AND  lupos.cloud.pig.udfs.BloomfilterUDF(bloom6.$0, $0) AND  lupos.cloud.pig.udfs.BloomfilterUDF(bloom7.$0, $0) AND  lupos.cloud.pig.udfs.BloomfilterUDF(bloom8.$0, $0);
INTERMEDIATE_BAG_1f = FILTER INTERMEDIATE_BAG_1 by lupos.cloud.pig.udfs.BloomfilterUDF(bloom0.$0, $0) AND  lupos.cloud.pig.udfs.BloomfilterUDF(bloom2.$0, $0) AND  lupos.cloud.pig.udfs.BloomfilterUDF(bloom3.$0, $0) AND  lupos.cloud.pig.udfs.BloomfilterUDF(bloom4.$0, $0) AND  lupos.cloud.pig.udfs.BloomfilterUDF(bloom5.$0, $0) AND  lupos.cloud.pig.udfs.BloomfilterUDF(bloom6.$0, $0) AND  lupos.cloud.pig.udfs.BloomfilterUDF(bloom7.$0, $0) AND  lupos.cloud.pig.udfs.BloomfilterUDF(bloom8.$0, $0);
INTERMEDIATE_BAG_2f = FILTER INTERMEDIATE_BAG_2 by lupos.cloud.pig.udfs.BloomfilterUDF(bloom1.$0, $0) AND  lupos.cloud.pig.udfs.BloomfilterUDF(bloom0.$0, $0) AND  lupos.cloud.pig.udfs.BloomfilterUDF(bloom3.$0, $0) AND  lupos.cloud.pig.udfs.BloomfilterUDF(bloom4.$0, $0) AND  lupos.cloud.pig.udfs.BloomfilterUDF(bloom5.$0, $0) AND  lupos.cloud.pig.udfs.BloomfilterUDF(bloom6.$0, $0) AND  lupos.cloud.pig.udfs.BloomfilterUDF(bloom7.$0, $0) AND  lupos.cloud.pig.udfs.BloomfilterUDF(bloom8.$0, $0);
INTERMEDIATE_BAG_3f = FILTER INTERMEDIATE_BAG_3 by lupos.cloud.pig.udfs.BloomfilterUDF(bloom1.$0, $0) AND  lupos.cloud.pig.udfs.BloomfilterUDF(bloom2.$0, $0) AND  lupos.cloud.pig.udfs.BloomfilterUDF(bloom0.$0, $0) AND  lupos.cloud.pig.udfs.BloomfilterUDF(bloom4.$0, $0) AND  lupos.cloud.pig.udfs.BloomfilterUDF(bloom5.$0, $0) AND  lupos.cloud.pig.udfs.BloomfilterUDF(bloom6.$0, $0) AND  lupos.cloud.pig.udfs.BloomfilterUDF(bloom7.$0, $0) AND  lupos.cloud.pig.udfs.BloomfilterUDF(bloom8.$0, $0);
INTERMEDIATE_BAG_4f = FILTER INTERMEDIATE_BAG_4 by lupos.cloud.pig.udfs.BloomfilterUDF(bloom1.$0, $0) AND  lupos.cloud.pig.udfs.BloomfilterUDF(bloom2.$0, $0) AND  lupos.cloud.pig.udfs.BloomfilterUDF(bloom3.$0, $0) AND  lupos.cloud.pig.udfs.BloomfilterUDF(bloom0.$0, $0) AND  lupos.cloud.pig.udfs.BloomfilterUDF(bloom5.$0, $0) AND  lupos.cloud.pig.udfs.BloomfilterUDF(bloom6.$0, $0) AND  lupos.cloud.pig.udfs.BloomfilterUDF(bloom7.$0, $0) AND  lupos.cloud.pig.udfs.BloomfilterUDF(bloom8.$0, $0);
